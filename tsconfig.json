{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": false,
    "removeComments": true,
    "alwaysStrict": true,
    "pretty": true,
    "outDir": "./Compiled",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "types.ts"
  ]
}